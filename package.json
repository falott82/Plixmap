    "@lexical/list": "^0.39.0",
    "@lexical/react": "^0.39.0",
    "@lexical/rich-text": "^0.39.0",
    "@lexical/selection": "^0.39.0",
    "@lexical/table": "^0.39.0",
    "@lexical/utils": "^0.39.0",
    "better-sqlite3": "^12.5.0",
    "classnames": "^2.5.1",
    "express": "^5.2.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.2.0",
    "konva": "^9.3.6",
    "lexical": "^0.39.0",
    "lucide-react": "^0.308.0",
    "nanoid": "^5.1.6",
    "nodemailer": "^7.0.12",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.7",
    "react-router-dom": "^6.30.3",
    "sonner": "^1.7.0",
    "speakeasy": "^2.0.0",
    "use-image": "^1.1.1",
    "ws": "^8.18.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.7",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.2.0"
  },
  "overrides": {
    "qs": "^6.14.1",
    "lodash": "^4.17.23"
  }
}

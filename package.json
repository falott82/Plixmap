{
  "name": "plixmap",
  "description": "Collaborative workspace planning with floor plans, desks, racks, and network links.",
  "version": "3.0.3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "node server/index.cjs",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit",
    "test": "node --test scripts/*.test.cjs",
    "test:e2e:smoke": "node scripts/e2e-smoke.cjs",
    "test:e2e:playwright": "npx playwright test",
    "lint:i18n": "node scripts/lint-i18n.cjs",
    "backup:db": "node scripts/db-backup.cjs",
    "sensitive:check": "node scripts/check-sensitive-data.cjs",
    "sensitive:check:staged": "node scripts/check-sensitive-data.cjs --staged",
    "hooks:install": "git config core.hooksPath .githooks",
    "release:check": "node scripts/check-release-version.cjs",
    "quality:check": "npm run sensitive:check && npm run lint && npm run test && npm run build && npm run release:check",
    "audit:prod": "npm audit --omit=dev --audit-level=high",
    "start": "node server/index.cjs"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@lexical/history": "^0.40.0",
    "@lexical/html": "^0.40.0",
    "@lexical/link": "^0.40.0",
    "@lexical/list": "^0.40.0",
    "@lexical/react": "^0.40.0",
    "@lexical/rich-text": "^0.40.0",
    "@lexical/selection": "^0.40.0",
    "@lexical/table": "^0.40.0",
    "@lexical/utils": "^0.40.0",
    "better-sqlite3": "^12.6.2",
    "classnames": "^2.5.1",
    "express": "^5.2.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.2.0",
    "konva": "^10.2.0",
    "lexical": "^0.40.0",
    "lucide-react": "^0.575.0",
    "nanoid": "^5.1.6",
    "nodemailer": "^8.0.1",
    "qrcode": "^1.5.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-konva": "^19.2.2",
    "react-router-dom": "^7.13.0",
    "sonner": "^2.0.7",
    "speakeasy": "^2.0.0",
    "use-image": "^1.1.1",
    "ws": "^8.19.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/node": "^20.10.7",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^7.3.0",
    "vite-plugin-pwa": "^1.2.0"
  },
  "overrides": {
    "qs": "^6.14.1",
    "lodash": "^4.17.23"
  }
}
